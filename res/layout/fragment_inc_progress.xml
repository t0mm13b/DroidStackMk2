<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/flGeneric_Progress" android:layout_width="match_parent"
	android:layout_height="match_parent" android:visibility="visible">

	<ProgressBar android:id="@+id/pgGeneric"
		style="?android:attr/progressBarStyleLarge" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_gravity="center_horizontal|center_vertical"
		android:indeterminate="true" android:visibility="visible" />
	<TextView android:id="@+id/tvGenericPg_NoData"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|center_vertical"
		android:padding="5dp" android:layout_margin="5dp"
		android:text="@string/fragGenQuestionsNoData"
		android:visibility="invisible" 
		android:textAppearance="?android:attr/textAppearanceLarge" />

</FrameLayout>
